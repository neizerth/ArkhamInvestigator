		"expo-splash-screen": "~0.30.10",
		"expo-status-bar": "~2.2.3",
		"expo-system-ui": "~5.0.11",
		"expo-web-browser": "~14.2.0",
		"fast-memoize": "^2.5.2",
		"fast-shuffle": "^6.1.1",
		"html-dom-parser": "^5.1.1",
		"html-react-parser": "^5.2.2",
		"i18next": "^25.2.1",
		"intl-pluralrules": "^2.0.1",
		"mathjs": "^14.3.0",
