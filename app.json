{
  "expo": {
    "name": "arkham-investigator",
    "slug": "arkham-investigator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-dark.png",
    "scheme": "arkham-investigator",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.arkhaminvestigator"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-light.png",
        "backgroundColor": "#24303C"
      },
      "package": "com.arkhaminvestigator"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-light.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#24303C"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
